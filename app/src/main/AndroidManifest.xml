<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.meindika80.bacatulisfilefilm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BuatDataFilm"
            android:label="@string/title_activity_buatdatafilm">
        </activity>
        <activity android:name=".LihatDataFilm"
            android:label="@string/title_activity_lihatdatafilm">
        </activity>
        <activity android:name=".UpdateDataFilm"
            android:label="@string/title_activity_updatedatafilm">
        </activity>
    </application>

</manifest>